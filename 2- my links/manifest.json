{

  "description": "Adds useful links for intenal use in Go-Labs company",
  "manifest_version": 2,
  "name": "Go-Labs",
  "version": "1.5",
  "homepage_url": "http://go-labs.net",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Go-Labs",
    "default_popup": "popup/popup.html"
  },

  "web_accessible_resources": [
    "images/*.png"
  ]

}
